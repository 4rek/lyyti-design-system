  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^14"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-a11y": "^6.3.1",
